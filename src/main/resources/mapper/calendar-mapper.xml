<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="calendar">

	<select id="test" resultType="string">
		select
			*
		from
			test
	</select>
	
	<insert id="insertCalendar">
		insert into
			calendar
		values (
			#{calendarId},
			#{calendarNo},
			#{subject},
			#{startDate},
			#{endDate},
			#{memo}
		)
	</insert>
	

</mapper>