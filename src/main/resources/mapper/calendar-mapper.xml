<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="calendar">
	<insert id="insertCal" parameterType="hashmap">
		insert into
			calendar
		values(
			#{id},
			#{title},
			#{start},
			#{end}
			)

	</insert>


</mapper>